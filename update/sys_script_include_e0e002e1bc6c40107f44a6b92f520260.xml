        <api_name>x_snc_jpmc.Wifi</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>WIFI Library</description>
        <name>Wifi</name>
        <script><![CDATA[var Wifi = Class.create();
Wifi.prototype = {
    initialize: function() {
    },
	getDevices: function() {
		var device = new GlideRecord("x_snc_jpmc_wifi");
		device.query();
		var responseValues = [];
		while(device.next()){
			responseValues.push({
				name: device.name+"",
				mac: device.mac+""
			});
		}
		return JSON.stringify(responseValues);
	},
    type: 'Wifi'
};]]></script>
